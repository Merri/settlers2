{
	"outputDirectory": "dist",
	"headers": [
		{
			"source": "/(favicon.ico|android-chrome-192x192.png|android-chrome-256x256.png|apple-touch-icon.png|browserconfig.xml|favicon-16x16.png|favicon-32x32.png|mstile-150x150.png|robots.txt|safari-pinned-tab.svg|site.webmanifest)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=3600"
				}
			]
		},
		{
			"source": "/(assets|fonts|icons)/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31556952, immutable"
				}
			]
		}
	],
	"redirects": [
		{
			"source": "/editor/world/",
			"destination": "https://bluebyte.settlers2.net/s2maps/uploads/",
			"permanent": true
		},
		{
			"source": "/world-editor/generator",
			"destination": "https://settlers2.net/map-generator",
			"permanent": true
		},
		{
			"source": "/archives/downloads",
			"destination": "https://settlers2.net/download",
			"permanent": true
		},
		{
			"source": "/archives/language-packs",
			"destination": "https://settlers2.net/download",
			"permanent": true
		},
		{
			"source": "/archives/map-generators",
			"destination": "https://settlers2.net/download",
			"permanent": true
		},
		{
			"source": "/archives/texture-pack-rusty-polar",
			"destination": "https://settlers2.net/download",
			"permanent": true
		},
		{
			"source": "/archives/texture-set-new-greenland",
			"destination": "https://settlers2.net/download",
			"permanent": true
		},
		{
			"source": "/archives",
			"destination": "https://settlers2.net/download",
			"permanent": true
		},
		{
			"source": "/cheats/world-map-file-format-wldswd",
			"destination": "/documentation/world-map-file-format-wldswd",
			"permanent": true
		}
	]
}
